   "source": [
    "\n",
    "from numpy import *\n",
    "set_printoptions(legacy = '1.25')\n",
    "\n",
    "\t\n",
    "def factorial(n):\n",
    "\tif n == 1: return 1\n",
    "\telse: return n * factorial(n-1)\n",
    "\t\n",
    "def stirling(n): return sqrt(2*pi*n) * (n/e)**n\n",
    "\t\n",
    "x = factorial(100)\n",
    "y = stirling(100)\n",
    "\t\n",
    "x/y, x-y\n"
   ]
  }
