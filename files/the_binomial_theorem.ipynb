   "source": [
    "\n",
    "from numpy import *\n",
    "\t\n",
    "N = 10\n",
    "Comb = zeros((N,N),dtype=int)\n",
    "Comb[0,0] = 1\n",
    "\t\n",
    "for n in range(1,N):\n",
    "\tComb[n,0] = Comb[n,n] = 1\n",
    "\tfor k in range(1,n): Comb[n,k] = Comb[n-1,k] + Comb[n-1,k-1]\n",
    "\t\n",
    "Comb\n"
   ]
