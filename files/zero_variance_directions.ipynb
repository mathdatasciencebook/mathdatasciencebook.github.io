    "\n",
    "from numpy import *\n",
    "from scipy.linalg import null_space\n",
    "\n",
    "from numpy.random import default_rng as rng\n",
    "samples = rng().random\n",
    "\n",
    "N, d = 20, 2\n",
    "dataset  = samples((N,d))\n",
    "\n",
    "def zero_variance(dataset):\n",
    "\tQ = cov(dataset.T)\n",
    "\treturn null_space(Q)\n",
    "\n",
    "zero_variance(dataset)\n"
   ]
